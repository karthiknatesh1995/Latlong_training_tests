<h1>Books Details</h1>
<table>
<tr>
<th>Accession Number</th>
<th>Title</th>
<th>Author</th>
<th>Edition</th>
<th>Publisher</th>
</tr>
<% @articles.each do |articles| %>
<tr>
<td><%= articles.acc_no %></td>
<td><%= articles.title %></td>
<td><%= articles.author %></td>
<td><%= articles.edition %></td>
<td><%= articles.publisher %></td>
</tr>
<% end %>
</table>
<%= link_to "New article", new_article_path %>